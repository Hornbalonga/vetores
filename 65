#include<stdio.h>

int main()
{
 int numero, maior, menor;
 int i;

 printf("Entre com o 1º número inteiro:\n ");
 scanf("%i", &numero);

 maior = numero;
 menor = numero;

 for(i=1; i<10; i++)
 {
 printf("\nEntre com o %dº numero inteiro: ",i+1);
 scanf("%i", &numero);

 if (numero > maior)
 maior = numero;
 else
 if (numero < menor)
 menor = numero;
 }

 printf("\nO menor numero entrado e: %d", menor);
 printf("\nO maior numero entrado e: %d", maior);

 return 0;
}
